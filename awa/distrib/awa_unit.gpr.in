with "util";
with "util_unit";
with "util_config";
with "el";
with "asf_core";
with "asf_unit";
with "ado";
with "awa";

project Awa_Unit is
   Version := "@AWA_VERSION@";

   for Library_Name use "awa_unit";
   for Library_Dir use Util_Config.Libdir;
   for Library_Kind use "LIBRARY_TYPE";

   for Source_Dirs use (Util_Config.Includedir & "/awa-unit");
   for Library_ALI_Dir use Util_Config.Alidir & "/awa-unit/";
   for Externally_Built use "true";
end Awa_Unit;
